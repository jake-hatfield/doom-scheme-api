	"bugs": {
		"url": "https://github.com/jake-hatfield/doom-scheme-api/issues"
	},
	"homepage": "https://github.com/jake-hatfield/doom-scheme-api#readme",
	"dependencies": {
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"module-alias": "^2.2.2",
		"multer": "^1.4.5-lts.1",
		"openai": "^3.2.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.15.9",
		"concurrently": "^7.6.0",
		"nodemon": "^2.0.22",
